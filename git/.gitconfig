[alias]
	st = status
	a = add
	aa = add --all
	ci = commit
	br = branch
	co = checkout
	di = diff
	lb = checkout -
	up = pull --rebase --autostash
[push]
	default = current
[pull]
	default = current
[branch]
	autosetuprebase = always
[core]
	excludesfile = ~/.gitignore
[credential]
	helper = osxkeychain
[diff]
	compactionHeuristic = true
[github]
	user = ttaylorr
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	required = true
	clean = git-lfs clean -- %f
